{
  "name": "stats-api",
  "version": "1.0.0",
  "description": "Simple stats API for use with Badges.io service",
  "main": "dist/server.js",
  "scripts": {
    "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "prestart:local": "npm run build",
    "start": "node ./dist/server.js",
    "start:local": "node ./dist/server.js",
    "test": "jest --coverage",
    "dev": "nodemon dist/server.js"
  },
  "keywords": [],
  "author": "Stjepan Mamusa",
  "license": "ISC",
  "dependencies": {
    "@influxdata/influxdb-client": "^1.28.0",
    "apicache": "^1.6.3",
    "axios": "^0.27.2",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-rate-limit": "^6.5.1",
    "got": "^11.8.5",
    "morgan": "^1.10.0",
    "winston": "^3.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/mocha": "^9.1.1",
    "babel-jest": "^28.1.3",
    "jest": "^28.1.3",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^2.0.16",
    "ts-jest": "^28.0.7",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^4.7.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.7"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverage": true,
    "testResultsProcessor": "jest-sonar-reporter",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ]
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
